
{% include 'rules.j2' %}

*filter
:INPUT {{ default_policies.input }} [0:0]
:FORWARD {{ default_policies.forward }} [0:0]
:OUTPUT {{ default_policies.output }} [0:0]

{% if default_policies.input != "ACCEPT" %}
-A INPUT -m conntrack --ctstate RELATED,ESTABLISHED -j ACCEPT
{{ ingress_accepts | accept_rule_set("INPUT") }}
{% endif %}

COMMIT
